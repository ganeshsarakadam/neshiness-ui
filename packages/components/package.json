  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0"
  }
