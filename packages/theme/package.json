{
  "name": "@neshiness-ui/theme",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./orange-black.css": "./styles/orange-black.css"
  },
  "files": ["dist", "styles", "README.md", "LICENSE"],
  "sideEffects": ["*.css"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts"
  },
  "devDependencies": {
    "tsup": "^8.0.2"
  }
}
