#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules/@storybook/cli/bin/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules/@storybook/cli/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules/@storybook/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules/@storybook/cli/bin/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules/@storybook/cli/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules/@storybook/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/@storybook+cli@8.6.14_@babel+preset-env@7.28.3_@babel+core@7.28.4__prettier@3.6.2/node_modules:/home/ganesh/kaboom/neshiness-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@storybook/cli/bin/index.cjs" "$@"
else
  exec node  "$basedir/../@storybook/cli/bin/index.cjs" "$@"
fi
